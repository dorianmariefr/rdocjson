<pre>
<%= JSON.pretty_generate(@toplevels.map do |toplevel|
  { name: toplevel.name, target: rdocize_toplevel(toplevel) }
end) %>
</pre>
<pre>
<%= JSON.pretty_generate(@classes_and_modules.map do |classmod|
  { name: classmod.full_name, target: rdocize_classmod(classmod) }
end) %>
</pre>
<pre>
<%= JSON.pretty_generate(@methods.map do |method|
  {
    name: "#{method.pretty_name} (#{method.parent.full_name})",
    target: "#{rdocize_classmod(method.parent)}/#{method.aref}"
  }
end) %>
</pre>
